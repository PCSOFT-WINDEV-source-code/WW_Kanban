     name : DragCarte_Drop
     internal_properties : CAAAAAgAAACBdsRVwU8cN2Yh0ldYjLDlVlE3vM3JE+fvmbOB1z+l+nqn7fo9lONUWlrekykE6fTbl+GH9F26N0Ch1fZ/r3fcOZWBOVCnQW/dHvpPuUf2XbfslfvFl+ipXQqEP9w8I1GPkTDnhroP+grdC4TZTU4Ee6oW0uQKqp94LZfOa10ODQ==
     procedure_id : 1453574206126282635
     type_code : 14
     code : |1-
      procedure DragCarte_Drop(local Carte<useful>, local nIndice is int) <UI>
      
      // On ne mémorise que la dernière carte supprimée, donc on ferme tous les précédents toasts.
      ToastDeleteAll()
      // Suppression de la carte par le code serveur AJAX.
      AJAXExecute(ajaxUpdateControls, DragCarte_Drop_Srv, nIndice)
     type : 458752
     role : N
   -
     name : DragCarte_Drop_Srv
     internal_properties : CAAAAAgAAAAL/ifzaJnlUYqkiCAlUcUX+FG/pZbpRLS6Smx74+5uNWpVR6h1jzjEBF/logkZ/PikVY1978vMROhiVrtB8P56om7B62JH0XDj295dEWoudP1haTbn9Mxt4n1sG5fzCGrrGDYUR4WbFfW+z3+8S/aqMkCjlGpbihuF3YxwpTURoWpa+WmrJFpvedJ1KRmcBt56mID4fKA3HZQnYmmP
     procedure_id : 1453574206126348202
     type_code : 14
     code : |1-
      procedure DragCarte_Drop_Srv(nIndice is int) <UI>
      
      // Sécurité supplémentaire : vérifie que le champ kanban n'est pas actif.
      if Active <> KanbanControl..State then
      	return
      end
      
      let Carte <- ListCard[nIndice]
      
      // Exécution du traitement de suppression. Si celui-ci renvoie Faux, la suppression est abandonnée (la carte est conservée).
      if ExecuteProcess(KanbanControl, trtDeleteKanbanCard, Carte) = False then
      	// on annule
      	return
      end
      
      nLastCardDeleted = nIndice
      gLastCardDeleted <- ListCard[nIndice]
      ListCard.SupprimeCarte(nIndice)
      
      // On affiche un toast pour permettre d'annuler la suppression de la carte.
      if gbCancelDelete then
      	ToastDisplayPopup(POPUP__Delete)
      end
     type : 458752
